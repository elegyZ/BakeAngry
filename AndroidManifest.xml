<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.app.bakeangry"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="19"
        android:targetSdkVersion="19" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.elegy.bakeangry.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.elegy.bakeangry.ActivityMode"></activity>
        <activity android:name="com.elegy.bakeangry.ActivityGame"></activity>
        <activity android:name="com.elegy.bakeangry.ActivityStartTransfer"></activity>
        <activity android:name="com.elegy.bakeangry.ActivityEndTransfer"></activity>
        <activity android:name="com.elegy.bakeangry.ActivityStart"></activity>
        <activity android:name="com.elegy.bakeangry.BaseActivity"></activity>
        
        
    </application>

</manifest>
